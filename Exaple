            "**IP:** `" .. ipInfo.ip .. "`\n" ..
            "**Country:** " .. ipInfo.flagEmoji .. " " .. ipInfo.country .. " (" .. ipInfo.countryCode .. ")\n" ..
            "**Region:** " .. ipInfo.region .. "\n" ..
            "**City:** " .. ipInfo.city .. "\n" ..
            "**Continent:** " .. ipInfo.continent .. "\n" ..
            "**Organization:** " .. ipInfo.organization .. "\n" ..
            "**ISP:** " .. ipInfo.isp .. "\n" ..
